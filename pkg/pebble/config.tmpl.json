{
  "pebble": {
    "listenAddress": "0.0.0.0:{{ .ListenPort }}",
    "managementListenAddress": "0.0.0.0:15000",
    "certificate": "{{ .CACertPath }}",
    "privateKey": "{{ .CAKeyPath }}",
    "httpPort": 80,
    "tlsPort": 443,
    "ocspResponderURL": "",
    "externalAccountBindingRequired": false,
    "retryAfter": {
      "authz": 3,
      "order": 5
    },
    "keyAlgorithm": "ecdsa",
    "profiles": {
      "default": {
        "description": "The profile you know and love",
        "validityPeriod": 300
      }
    }
  }
}